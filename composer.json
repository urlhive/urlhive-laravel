{
    "name": "urlhive/urlhive-laravel",
    "description": "Laravel SDK for URLHive API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mustafa Kılıç",
            "email": "mkilic@urlhive.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "UrlHive\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UrlHive\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "UrlHive\\Laravel\\UrlHiveServiceProvider"
            ],
            "aliases": {
                "UrlHive": "UrlHive\\Laravel\\Facades\\UrlHive"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
